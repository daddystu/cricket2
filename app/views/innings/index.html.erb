<h1>Listing innings</h1>

<table>
  <tr>
    <th>Match</th>
    <th>Team</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @innings.each do |inning| %>
  <tr>
    <td><%= inning.match_id %></td>
    <td><%= inning.team_id %></td>
    <td><%= link_to 'Show', inning %></td>
    <td><%= link_to 'Edit', edit_inning_path(inning) %></td>
    <td><%= link_to 'Destroy', inning, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Inning', new_inning_path %>
