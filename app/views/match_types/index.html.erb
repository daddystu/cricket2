<h1>Listing match_types</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @match_types.each do |match_type| %>
  <tr>
    <td><%= match_type.name %></td>
    <td><%= link_to 'Show', match_type %></td>
    <td><%= link_to 'Edit', edit_match_type_path(match_type) %></td>
    <td><%= link_to 'Destroy', match_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Match type', new_match_type_path %>
